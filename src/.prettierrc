{
    "jsxSingleQuote": true,
    "singleQuote": true,
    "semi": true,
    "tabWidth": 2,
    "bracketSameLine": false,
    "useTabs": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "trailingComma": "none",
    "printWidth": 200,
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
