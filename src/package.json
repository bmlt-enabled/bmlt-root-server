        "@tsconfig/svelte": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.8.0",
        "sass": "^1.55.0",
        "smui-theme": "^7.0.0-beta.0",
        "svelte": "^3.50.1",
