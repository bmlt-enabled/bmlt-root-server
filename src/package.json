{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && rm -rf public/_app && mv build/_app public/ && mv build/favicon.png public/ && rm -rf build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
